{
  "name": "VNProducer",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "fnando.linter",
        "prisma.prisma",
        "mikestead.dotenv",
        "eamodio.gitlens"
      ]
    }
  },
  "forwardPorts": [3000, 5000],
  "postCreateCommand": "yarn install",
  "remoteUser": "node",
  "mounts": [
    "source=${env:HOME}/.ssh,id=sshkey,target=/home/node/.ssh,type=bind,consistency=cached"
  ]
}
